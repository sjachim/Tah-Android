<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <RelativeLayout
        android:id="@+id/mainrelative"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:gravity="center_vertical|center_horizontal">

        <util.NumberProgressBar
            android:id="@+id/anologpina0"
            style="@style/NumberProgressBar_Funny_Orange"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:padding="20dp"
            custom:progress="0" />

        <util.NumberProgressBar
            android:id="@+id/anologpina1"
            style="@style/NumberProgressBar_Funny_Orange"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/anologpina0"
            android:layout_margin="10dp"
            android:padding="20dp"
            custom:progress="0" />

        <util.NumberProgressBar
            android:id="@+id/anologpina2"
            style="@style/NumberProgressBar_Funny_Orange"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/anologpina1"
            android:layout_margin="10dp"
            android:padding="20dp"
            custom:progress="0" />

        <util.NumberProgressBar
            android:id="@+id/anologpina3"
            style="@style/NumberProgressBar_Funny_Orange"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/anologpina2"
            android:layout_margin="10dp"
            android:padding="20dp"
            custom:progress="0" />


        <util.NumberProgressBar
            android:id="@+id/anologpina4"
            style="@style/NumberProgressBar_Funny_Orange"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/anologpina3"
            android:layout_margin="10dp"
            android:padding="20dp"
            custom:progress="0" />


        <util.NumberProgressBar
            android:id="@+id/anologpina5"
            style="@style/NumberProgressBar_Funny_Orange"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/anologpina4"
            android:layout_margin="10dp"
            android:padding="20dp"
            custom:progress="0" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="A0"
            android:textColor="#ffffff"
            android:id="@+id/textView8"
            android:layout_alignTop="@+id/anologpina0"
            android:layout_alignStart="@+id/anologpina2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="A1"
            android:textColor="#ffffff"
            android:id="@+id/textView9"
            android:layout_alignTop="@+id/anologpina1"
            android:layout_alignStart="@+id/textView8" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="A2"
            android:textColor="#ffffff"
            android:id="@+id/textView10"
            android:layout_alignTop="@+id/anologpina2"
            android:layout_alignStart="@+id/anologpina2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="A3"
            android:textColor="#ffffff"
            android:id="@+id/textView11"
            android:layout_alignTop="@+id/anologpina3"
            android:layout_alignStart="@+id/anologpina3" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="A3"
            android:textColor="#ffffff"
            android:id="@+id/textView12"
            android:layout_alignTop="@+id/anologpina4"
            android:layout_alignStart="@+id/anologpina4" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="A5"
            android:textColor="#ffffff"
            android:id="@+id/textView13"
            android:layout_alignTop="@+id/anologpina5"
            android:layout_alignStart="@+id/anologpina5" />
    </RelativeLayout>
</LinearLayout>
